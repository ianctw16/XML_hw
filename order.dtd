<?xml version="1.0"?>
<!DOCTYPE order_list [
    <!ELEMENT order_stock   (order_id, order_status, payment_method, customer_name, custmer_id, product_1_name, product_1_price, product_2_name, product_2_price)>
    
    <!ELEMENT order_scheduled   (order_id, order_status, payment_method, customer_name, custmer_id, product_1_name, product_1_price, product_2_name, product_2_price)>
    
    <!ELEMENT order_id  (#PCDATA)>
    <!ELEMENT order_status  (#PCDATA)>
    <!ATTLIST payment_method  (credit_card|cash) "cash">
