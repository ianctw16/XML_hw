    <!ELEMENT customer_id    (#PCDATA)>
    <!ELEMENT product_1_name    (#PCDATA)>
    <!ELEMENT product_1_price  (#PCDATA)>
    <!ELEMENT product_2_name  (#PCDATA)>
    <!ELEMENT product_2_price    (#PCDATA)>
